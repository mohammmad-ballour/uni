<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dbms_project.LoginController">
   <children>
      <Label layoutX="14.0" layoutY="25.0" text="You are an adminstarator?" />
      <TextField fx:id="userID" layoutX="78.0" layoutY="67.0" promptText="ID" style="-fx-border-color: #0559ff; -fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px;" />
      <PasswordField fx:id="password" layoutX="78.0" layoutY="184.0" promptText="password" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: #0559ff;" />
      <CheckBox fx:id="checkButton" layoutX="165.0" layoutY="25.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="27.0" />
      <TextField fx:id="username" layoutX="78.0" layoutY="125.0" promptText="Name" style="-fx-border-color: #0559ff; -fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px;" />
      <Button layoutX="109.0" layoutY="244.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="login" />
   </children>
</AnchorPane>
